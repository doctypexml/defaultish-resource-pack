{
    "multipart": [
        {   "apply": { "model": "block/light_gray_stained_glass_pane_post" }},
        {   "when": { "north": true },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_side" },
              { "model": "block/light_gray_stained_glass_pane_side", "y": 180}
            ]
        },
        {   "when": { "east": true },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_side", "y": 90 },
              { "model": "block/light_gray_stained_glass_pane_side", "y": 270 }
            ]
        },
        {   "when": { "south": true },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_side_alt" },
              { "model": "block/light_gray_stained_glass_pane_side_alt", "y": 180}
            ]
        },
        {   "when": { "west": true },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_side_alt", "y": 90 },
              { "model": "block/light_gray_stained_glass_pane_side_alt", "y": 270 }
            ]
        },
        {   "when": { "north": false },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_noside" },
              { "model": "block/light_gray_stained_glass_pane_noside", "y": 180 }
            ]
        },
        {   "when": { "east": false },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_noside_alt" },
              { "model": "block/light_gray_stained_glass_pane_noside_alt", "y": 180 }
            ]
        },
        {   "when": { "south": false },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_noside_alt", "y": 90 },
              { "model": "block/light_gray_stained_glass_pane_noside_alt", "y": 270 }
            ]
        },
        {   "when": { "west": false },
            "apply": [
              { "model": "block/light_gray_stained_glass_pane_noside", "y": 90 },
              { "model": "block/light_gray_stained_glass_pane_noside", "y": 270 }
            ]
        }
    ]
}
